include_rules

: LispBridge.cpp |> !cxx |> {liblispSupport}
: LispEnvironment.cpp |> !cxx |> {liblispSupport}
: Binding.cpp |> !cxx |> {liblispSupport}

: {liblispSupport} |> !ar |> liblispSupport.a

: foreach *.lisp |> !ecl |> {liblispLogic}
: {liblispLogic} |> !eclar |> liblispLogic.a

.gitignore
